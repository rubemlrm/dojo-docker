[traefik-auth]
enabled = true
chain = DOCKER-USER
port = http,https
filter = traefik-auth
logpath = {{ docker_configs.config_path }}/volumes/traefik/logs/access.log

[traefik-botsearch]
enabled = true
chain = DOCKER-USER
port = http,https
filter = traefik-botsearch
logpath = {{ docker_configs.config_path }}/volumes/traefik/logs/access.log
